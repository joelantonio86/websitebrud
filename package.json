{
  "name": "banda-racional-site",
  "version": "2.0.0",
  "description": "Site oficial da Banda Racional Universo em Desencanto - React + TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:gh-pages": "vite build && node scripts/copy-404.js",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "banda",
    "racional",
    "cultura-racional",
    "musica",
    "react",
    "typescript"
  ],
  "author": "Banda Racional Universo em Desencanto",
  "license": "MIT",
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-simple-maps": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
